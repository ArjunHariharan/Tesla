<Window x:Class="Tesla.BalancePaidDateWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Balance Paid Date" Height="200" Width="400">
    <Grid>
        <Label Content="Balance Paid On" HorizontalAlignment="Left" Margin="49,46,0,0" VerticalAlignment="Top"/>
        <DatePicker x:Name="DatePicker" HorizontalAlignment="Left" Margin="210,48,0,0" VerticalAlignment="Top"/>
        <Button x:Name="OkButton" Content="Ok" HorizontalAlignment="Left" Margin="70,95,0,0" VerticalAlignment="Top" Width="75" Click="OkButton_Click"/>
        <Button x:Name="CancelButton" HorizontalAlignment="Left" Margin="228,95,0,0" VerticalAlignment="Top" Width="75" Height="20" Content="Cancel" Click="CancelButton_Click"/>
    </Grid>
</Window>
