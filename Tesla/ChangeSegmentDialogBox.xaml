<Window x:Class="Tesla.ChangeSegmentDialogBox"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Change Segment" Height="203.008" Width="368.797" WindowStartupLocation="CenterScreen" ResizeMode="NoResize" Loaded="Window_Loaded">
    <Grid>
        <Label Content="Select The Segment" HorizontalAlignment="Left" Margin="19,55,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="SegmentList" HorizontalAlignment="Left" Margin="190,61,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="SegmentList_SelectionChanged"/>
        <Button x:Name="OkButton" Content="Ok" HorizontalAlignment="Left" Margin="59,125,0,0" VerticalAlignment="Top" Width="75" Click="OkButton_Click"/>
        <Button x:Name="CancelButton" Content="Cancel" HorizontalAlignment="Left" Margin="214,125,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="0.522,-0.649" Click="CancelButton_Click"/>

    </Grid>
</Window>
